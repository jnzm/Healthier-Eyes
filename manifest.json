{
    "name": "Healthier Eyes",
    "description": "An all-in-one extension that exercises your eye muscle and neck muscle",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Lucia Lee",
    "icons": {"128": "img/icon_128.png"},
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "alarm",
        "idle"
    ]
}